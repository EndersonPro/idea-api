{
    "family": "EndersonPro",
    "executionRoleArn": "arn:aws:iam::112233445566:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "idea_api-container",
            "image": "112233445566.dkr.ecr.us-east-2.amazonaws.com/dcdojo-amine:4622390a2b34f72y5f389cb62128a305641942cd",
            "cpu": 512,
            "portMappings": [
              {
                "hostPort": 8888,
                "protocol": "tcp",
                "containerPort": 8888
              }
            ],
            "memory": 1024,
            "essential": true,
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "/ecs/dcdojo",
                "awslogs-region": "us-east-2",
                "awslogs-stream-prefix": "ecs"
              }
            }
        }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048"
  }